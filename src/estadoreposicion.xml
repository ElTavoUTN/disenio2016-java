<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Clases.EstadoReposicion" table="estadoreposicion">
      
        <id name="OID" type="string">
            <column name="OIDEstadoReposicion"/>
            <generator class="uuid2"/>
        </id>
        <property name="codigoEstadoReposicion" type="int">
            <column name="codigoEstadoReposicion"/>
        </property>
      
        <property name="nombreEstadoReposicion" type="string">
            <column name="nombreEstadoReposicion"/>
        </property>
      
    </class>
      
</hibernate-mapping>
