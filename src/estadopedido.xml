<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Clases.EstadoPedido" table="estadopedido">
          
        <id name="OID" type="string">
            <column name="OIDEstadoPedido"/>
            <generator class="uuid2"/>
        </id>
        
        <property name="codigoEstadoPedido" type="int">
            <column name="codigoEstadoPedido"/>
        </property>
        
        <property name="nombreEstadoPedido" type="string">
            <column name="nombreEstadoPedido"/>
        </property>
                      
    </class>
     
</hibernate-mapping>
