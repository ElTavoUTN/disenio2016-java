<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Clases.Parametro" table="parametro">
  
        <id name="codigoParametro" type="string">
            <column name="codigoParametro"/>
            <generator class="uuid2"/>
        </id>
        
        <property name="descripcionParametro" type="string">
            <column name="descripcionParametro"/>
        </property>
        
        <property name="valorParametro" type="string">
            <column name="valorParametro"/>
        </property>
     
    </class>
  
</hibernate-mapping>
